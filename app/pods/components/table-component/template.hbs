
<table class="table">
  <thead>
    <tr>
      {{#each headers as |header|}}
        <th>{{header}}</th>
      {{/each}}
    </tr>
  </thead>
  <tbody>
    {{#each tableColumn as |column|}}
    <tr>
      <td>{{column.count}}</td>
      <td>{{column.cost}}</td>
      {{#each-in column.events as |title event|}}
      <td>{{event}}</td>
      {{/each-in}}
    </tr>
    {{/each}}
  </tbody>
</table>